<!-- display user's fav table -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Favorites</title>
</head>
<body>
  <%- include('partials/_header') %>

  <table>
    <tr>

    </tr>
    <tr>
      <% for (let fave of faves) {%>
      <td><%= fave.game_title %></td>
      <td></td>
      <td><a href="/games/<%= fave.game_id %>"><img src="<%= fave.game_photo %>"></a></td>
      <% } %>
      </tr>
  </table>
</body>
</html>
